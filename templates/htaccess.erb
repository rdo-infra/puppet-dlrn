<% if @distro_branch == "master" %>
RedirectMatch "^/(.*)/current-passed-ci(.*)" "http://buildlogs.centos.org/centos/7/cloud/x86_64/rdo-trunk-master-tested/$2" 
RedirectMatch "^/(.*)/current-tripleo(.*)" "http://buildlogs.centos.org/centos/7/cloud/x86_64/rdo-trunk-master-tripleo/$2"
<% else %>
RedirectMatch "^/(.*)/current-passed-ci(.*)" "http://buildlogs.centos.org/centos/7/cloud/x86_64/rdo-trunk-<%= @release %>-tested/$2"
<% end %>
